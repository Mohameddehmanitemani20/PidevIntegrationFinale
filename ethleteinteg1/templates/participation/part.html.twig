{% extends 'back.html.twig' %}

{% block title %}User index{% endblock %}

{% block body %}
    <h1>User index</h1>

    <table class="table">
        <thead>
            <tr>
              <th>Username</th>
               <th>Prenom</th>
               
                <th>Nom</th>
          
              
            
              
                <th>Email</th>
              
             
            </tr>
        </thead>
        <tbody>
        {% for user in participants %}
            <tr>
               <td>{{ user.username }}</td>
                
                     <td>{{ user.prenom }}</td>
                    <td>{{ user.nom }}</td>
             
           
           
           
                <td>{{ user.email }}</td>
           
     
             
            </tr>
        {% else %}
            <tr>
                <td colspan="13">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('formation_index') }}">back to list</a></button>
{% endblock %}
